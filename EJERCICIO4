using System;
					
public class Program
{
	public static void Main()
	{
		int a = 5, b = 6;
            Console.WriteLine("El numeros antes de intercambiar son ");
            Console.WriteLine("numero1 = " + a.ToString());
            Console.WriteLine("numero2 = " + b.ToString());

            a = a + b - 1;
            b = a - b + 1;
            a = a - b + 1;
            
            Console.WriteLine("Ya se intercambiaron, ahora");
            Console.WriteLine("numero1 = " + a.ToString());
            Console.WriteLine("numero2 = " + b.ToString());
            
       
	}
}
